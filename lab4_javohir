    char c;
    cout << "Enter a character: ";
    cin >> c;
    if ( c == 'g') {
        cout << "Go!";
    }
    else if ( c == 'y') {
        cout << "Get ready!";
    }
    else if ( c == 'r') {
        cout << "Stop!";
    }
    return 0;
