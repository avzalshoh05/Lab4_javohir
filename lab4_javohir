using namespace std;
int main() {
    int n;
    float f;
    cout << "Enter a weight and cost: ";
    cin >> n >> f;
    int n2;
    float f2;
    cout << "Enter another weight and cost: ";
    cin >> n2 >> f2;
    if (f/n > f2/n2) {
        cout << "Package 1 has a better price";
    }
    else if (f/n < f2/n2) {
        cout << "Package 2 has a better price";
    }
    else {
        cout << "Two packages have the same price";
    }
    return 0;
}
*/

