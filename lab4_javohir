//Problem 10

/*
#include <iostream>
using namespace std;
int main() {
    int n;
    cout << "Enter a number: ";
    cin >> n;
    if (n <= 100 && n >= 90) {
        cout << "A";
    }
    else if (n <= 89 && n >= 80) {
        cout << "B";
    }
    else if (n <= 79 && n >= 70) {
        cout << "C";
    }
    else if (n <= 69 && n >= 60) {
        cout << "D";
    }
    else {
        cout << "F";
    }
    return 0;
}
*/

//Problem 11

/*
#include <iostream>
using namespace std;
int main() {
    int n, n2;
    cout << "Enter 2 number: ";
    cin >> n >> n2;
    if (n % n2 == 0) {
        cout << n << " is divisible by " << n2 << endl;
    }
    else {
        cout << n << " is not divisible by " << n2 << endl;
    }
    return 0;
}
*/

//Problem 12

/*
#include <iostream>
using namespace std;
int main() {
    int n, n2, n3;
    cout << "Enter 3 number: ";
    cin >> n >> n2 >> n3;
    if (n+n2 > n3 && n+n3 > n2 && n2+n3 > n) {
        cout << "Th triangle is valid";
    }
    else {
        cout << "Th triangle is not valid";
    }
    return 0;
}
*/

